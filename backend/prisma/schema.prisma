generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id                     Int     @id @default(autoincrement())
  nombre                 String  @unique
  dinero                 Decimal @default(0)
  personaje_seleccionado String
}

model Personaje {
  id     Int     @id @default(autoincrement())
  nombre String  @unique
  ki     Int
  precio Decimal
}
